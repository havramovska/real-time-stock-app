{
  "name": "websocket-server",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/websocket-server",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node apps/websocket-server/server.js",
        "cwd": "."
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nodemon apps/websocket-server/server.js",
        "cwd": "."
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'WebSocket server is ready to run'"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  },
  "tags": []
} 